package com.mbgo.mybatis.mbstore.bean;


public class MallThemeList {
    private String code;
    private Integer showType;
    private Integer isPaging;
    
    
	public MallThemeList() {
		super();
	}


	public MallThemeList(Integer id, String code, Integer showType,
			Integer isPaging) {
		this.code = code;
		this.showType = showType;
		this.isPaging = isPaging;
	}


	public String getCode() {
		return code;
	}


	public void setCode(String code) {
		this.code = code;
	}


	public Integer getShowType() {
		return showType;
	}


	public void setShowType(Integer showType) {
		this.showType = showType;
	}


	public Integer getIsPaging() {
		return isPaging;
	}


	public void setIsPaging(Integer isPaging) {
		this.isPaging = isPaging;
	}
    
    
}